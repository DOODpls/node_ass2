<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <link rel="stylesheet" href="css/template.css">
  <link rel="stylesheet" href="fontawesome-free-5.10.2-web/css/all.css">
  <link rel="stylesheet" href="css/gallery.css">
  <title><%= title %></title>
  <link rel="icon" href="./images/favicon.ico">
</head>
<body>
    <div class="wrapper">
      <%- include partials/header %>
        <div class="MainContainer">
          <div class="ParallaxContainer">
            <h1 id="hero">
              <%= hero %>
            </h1>
          </div>
          <div class="ContentContainer">
            <div class="contentgrd">
              <div id="gal1">
                <h1 class="galtitle"><%= album1.title %></h1>
              <div class="gridauto">
                <% for(i=0;i < 10; i++){ console.log(album1.images[i].src)%>
                  <img src="<%= album1.images[i].src %>" alt="<%= album1.images[i].atl %>">
                <% } %>
              </div>
              </div>
              <div id="gal2">
                <h1 class="galtitle"><%= album2.title %></h1>
                <div class="gridauto">
                  <% for(i=0;i < 10; i++){ console.log(album2.images[i].src)%>
                    <img src="<%= album2.images[i].src %>" alt="<%= album2.images[i].atl %>">
                  <% } %>
                </div>
            </div>
              <div id="gal3">
                <h1 class="galtitle"><%= album3.title %></h1>
                <div class="gridauto">
                  <% for(i=0;i < 10; i++){ console.log(album3.images[i].src)%>
                    <img src="<%= album3.images[i].src %>" alt="<%= album3.images[i].atl %>">
                  <% } %>
                </div>
              </div>
              <div id="gal4">
                <h1 class="galtitle"><%= album4.title %></h1>
                <div class="gridauto">
                  <% for(i=0;i < 10; i++){ console.log(album4.images[i].src)%>
                    <img src="<%= album4.images[i].src %>" alt="<%= album4.images[i].atl %>">
                  <% } %>
                </div>
              </div>
            </div>
        </div>
        <%- include partials/footer %>
      </div>
    </div>
</body>
</html>